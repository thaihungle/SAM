{
  "model": {
    "entity_size": 90,
    "hidden_size": 80,
    "role_size": 20,
    "init_limit": 0.10,
    "LN": true
  },
  "trainer":{
    "steps": 1000000,
    "epochs": 500,
    "batch_size": 128,
    "validation_step": 200,
    "logging_step": 50,
    "use_cuda": true
  },
  "data":{
    "data_path": "./babi/data/en-valid-10k",
    "task-id": "all"
  },
  "optimizer":{
    "lr": 0.0006,
    "beta1": 0.9,
    "beta2": 0.99,
    "max_gradient_norm": 5,
    "warm_up": true,
    "warm_up_steps": 1,
    "warm_up_factor": 0.1,
    "decay": true,
    "decay_thr": 0.1,
    "decay_factor": 0.5
  }
}